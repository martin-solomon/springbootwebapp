{
    "family": "springbootwebapp",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "springbootwebapp",
            "cpu": 1024,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080
                }
            ]
        }
    ]
}
